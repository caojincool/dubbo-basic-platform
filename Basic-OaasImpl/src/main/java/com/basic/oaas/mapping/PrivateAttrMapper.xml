<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.basic.oaas.dao.PrivateAttrMapper">
    <resultMap id="PrivateAttr" type="com.basic.oaas.model.PrivateAttr">
		<id property="attrId" column="ATTR_ID" jdbcType="NUMERIC"/>
		<result property="privateId" column="PRIVATE_ID" jdbcType="NUMERIC"/>
		<result property="menuId" column="MENU_ID" jdbcType="NUMERIC"/>
		<result property="attrCode" column="ATTR_CODE" jdbcType="VARCHAR"/>
		<result property="attrName" column="ATTR_NAME" jdbcType="VARCHAR"/>
		<result property="state" column="STATE" jdbcType="VARCHAR"/>
		<result property="createUserId" column="CREATE_USER_ID" jdbcType="NUMERIC"/>
		<result property="createTime" column="CREATE_TIME" jdbcType="TIMESTAMP"/>
		<result property="modifyUserId" column="MODIFY_USER_ID" jdbcType="NUMERIC"/>
		<result property="modifyTime" column="MODIFY_TIME" jdbcType="TIMESTAMP"/>
		<result property="remarks" column="REMARKS" jdbcType="VARCHAR"/>
		<result property="openStatus" column="OPEN_STATUS" jdbcType="NUMERIC"/>
		<result property="openDate" column="OPEN_DATE" jdbcType="TIMESTAMP"/>
		<result property="stopDate" column="STOP_DATE" jdbcType="TIMESTAMP"/>
    </resultMap>

    <insert id="create" parameterType="com.basic.oaas.model.PrivateAttr">
        INSERT INTO OAAS_PRIV_ATTR
        (ATTR_ID,PRIVATE_ID,MENU_ID,ATTR_CODE,ATTR_NAME,STATE,CREATE_USER_ID,CREATE_TIME,MODIFY_USER_ID,MODIFY_TIME,REMARKS,OPEN_STATUS,OPEN_DATE,STOP_DATE)
        VALUES
        (#{attrId,jdbcType=NUMERIC}, #{privateId,jdbcType=NUMERIC}, #{menuId,jdbcType=NUMERIC}, #{attrCode,jdbcType=VARCHAR}, #{attrName,jdbcType=VARCHAR}, #{state,jdbcType=VARCHAR}, #{createUserId,jdbcType=NUMERIC}, #{createTime,jdbcType=TIMESTAMP}, #{modifyUserId,jdbcType=NUMERIC}, #{modifyTime,jdbcType=TIMESTAMP}, #{remarks,jdbcType=VARCHAR}, #{openStatus,jdbcType=NUMERIC}, #{openDate,jdbcType=TIMESTAMP}, #{stopDate,jdbcType=TIMESTAMP})
    </insert>

    <select id="get"   parameterType="java.lang.Long" resultMap="PrivateAttr">
        SELECT * FROM OAAS_PRIV_ATTR
        WHERE state = '10A' AND
    ATTR_ID=#{attrId}
    </select>

    <select id="getAll"    resultMap="PrivateAttr">
        SELECT * FROM OAAS_PRIV_ATTR
        WHERE state = '10A'
    </select>

    <select id="query" parameterType="java.util.Map" resultMap="PrivateAttr">
        SELECT * FROM OAAS_PRIV_ATTR
        <where>
            state = '10A'
            <if test="whereSql!=null">
                AND ${whereSql}
            </if>
        </where>
        <if test="orderBySql!=null">
            ORDER BY ${orderBySql}
        </if>
        <if test="orderBySql==null">
            ORDER BY ATTR_ID DESC
        </if>
    </select>

    <update id="update" parameterType="com.basic.oaas.model.PrivateAttr">
        UPDATE OAAS_PRIV_ATTR SET
            PRIVATE_ID=#{privateId,jdbcType=NUMERIC},
            MENU_ID=#{menuId,jdbcType=NUMERIC},
            ATTR_CODE=#{attrCode,jdbcType=VARCHAR},
            ATTR_NAME=#{attrName,jdbcType=VARCHAR},
            STATE=#{state,jdbcType=VARCHAR},
            CREATE_USER_ID=#{createUserId,jdbcType=NUMERIC},
            CREATE_TIME=#{createTime,jdbcType=TIMESTAMP},
            MODIFY_USER_ID=#{modifyUserId,jdbcType=NUMERIC},
            MODIFY_TIME=#{modifyTime,jdbcType=TIMESTAMP},
            REMARKS=#{remarks,jdbcType=VARCHAR},
            OPEN_STATUS=#{openStatus,jdbcType=NUMERIC},
            OPEN_DATE=#{openDate,jdbcType=TIMESTAMP},
            STOP_DATE=#{stopDate,jdbcType=TIMESTAMP}
        WHERE state = '10A' AND
    ATTR_ID=#{attrId}
    </update>
    
    <delete id="remove" parameterType="java.lang.Long">
        UPDATE OAAS_PRIV_ATTR SET  state = '10X'
        WHERE
    ATTR_ID=#{attrId}
    </delete>


</mapper>
DROP TABLE IF EXISTS `pub_gid_server`;
 
CREATE TABLE IF NOT EXISTS  `PUB_GID_SERVER`(
	GID_CODE VARCHAR(64) NOT NULL,
	GID_NAME VARCHAR(64),
	IS_USE INT(11),
	CURR_VALUE INT(11),
	CACHE_SIZE INT(11),
	CLIENT_CACHE_SIZE INT(11),
	INCREAMENT_BY INT(11),
	REMARK VARCHAR(64)
) ENGINE=INNODB DEFAULT CHARSET=UTF8;